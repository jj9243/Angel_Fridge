<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    >

    <EditTextPreference
        android:title="User"
        android:key="user"
        android:summary="입력해주세요"
        />

    <SwitchPreference
        android:title="Alarm"
        android:key="switch"
        />

    <ListPreference
        android:entries="@array/notify"
        android:entryValues="@array/notify"
        android:summary="Anywhere"
        android:key="location"
        android:title="Location"
        android:defaultValue="Anywhere"
        />

    <EditTextPreference
        android:title="Date / Time"
        android:key="notification"
        android:summary="setting1"
        />


</PreferenceScreen>